{
  "name": "docs-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "FAST_REFRESH=false docusaurus start --host 0.0.0.0 --poll",
    "start:fast": "FAST_REFRESH=false TSC_COMPILE_ON_ERROR=true docusaurus start --host 0.0.0.0 --no-open",
    "build": "npm run api:generate && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "npm run api:generate && docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "api:generate": "pydoc-markdown && rm -f docs/api/reference/feax/experimental/feax_form_compiler.md && find docs/api/reference -name '*.md' -exec sed -i 's/{\\([^}]*\\)}/{`\\1`}/g' {} \\; && python3 format_examples.py && python3 format_parameters.py && find docs/api/reference -name '*.md' -exec sed -i 's/[[:space:]]*$//' {} \\;",
    "api:clean": "rm -rf docs/api/reference"
  },
  "dependencies": {
    "@docusaurus/core": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.2",
    "@docusaurus/tsconfig": "3.9.2",
    "@docusaurus/types": "3.9.2",
    "@swc/core": "^1.15.0",
    "swc-loader": "^0.2.6",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  }
}
